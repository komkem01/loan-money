{
  "version": 2,
  "functions": {
    "api/index.go": {
      "runtime": "go1.x"
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api/index.go"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.go"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ]
}
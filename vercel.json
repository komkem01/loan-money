{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/main.go"
    },
    {
      "src": "/api/(.*)",
      "dest": "/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "/main.go"
    }
  ],
  "env": {
    "GO_VERSION": "1.21"
  }
}
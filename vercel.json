{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/index.go"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.go"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "functions": {
    "api/index.go": {
      "runtime": "go1.x"
    }
  }
}
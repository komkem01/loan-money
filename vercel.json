{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/index.go"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.go"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "env": {
    "GO_VERSION": "1.21"
  }
}